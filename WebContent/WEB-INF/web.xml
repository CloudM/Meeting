<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>CloudM</display-name>
  

  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
        <listener>  
        <listener-class>com.function.Sessions</listener-class>  
    </listener>
  

  <servlet>
    <servlet-name>LoginAndRegister</servlet-name>
    <servlet-class>com.action.LoginAndRegister</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginAndRegister</servlet-name>
    <url-pattern>/LoginAndRegister</url-pattern>
  </servlet-mapping>
  
    <servlet>
    <servlet-name>CreateAndReleaseMeeting</servlet-name>
    <servlet-class>com.action.MeetingAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CreateAndReleaseMeeting</servlet-name>
    <url-pattern>/CreateAndReleaseMeeting</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>ApplyAndCancelMeeting</servlet-name>
    <servlet-class>com.action.ApplyAndCancel</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ApplyAndCancelMeeting</servlet-name>
    <url-pattern>/ApplyAndCancelMeeting</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>ShowApply</servlet-name>
    <servlet-class>com.action.ShowApply</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ShowApply</servlet-name>
    <url-pattern>/ShowApply</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>HandleApply</servlet-name>
    <servlet-class>com.action.HandleApply</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HandleApply</servlet-name>
    <url-pattern>/HandleApply</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>ResetUserServlet</servlet-name>
    <servlet-class>com.action.ResetServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ResetUserServlet</servlet-name>
    <url-pattern>/ResetUserServlet</url-pattern>
  </servlet-mapping>
  
    <servlet>
    <servlet-name>SelectMeeting</servlet-name>
    <servlet-class>com.action.ShowMeetingInfo</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SelectMeeting</servlet-name>
    <url-pattern>*.do</url-pattern>
  </servlet-mapping>
  
      <servlet>
    <servlet-name>SearchMeeting</servlet-name>
    <servlet-class>com.action.SearchMeeting</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SearchMeeting</servlet-name>
    <url-pattern>*.searchMeeting</url-pattern>
  </servlet-mapping>
  
</web-app>